programa {
  funcao inicio() {
    inteiro idade, contribuicao
    escreva("Digite a idade: ")
    leia(idade)
    escreva("Digite quantos anos de contribuição: ")
    leia(contribuicao)

    se (idade > 60 ou contribuicao > 30 )
    escreva("Pode se aposentar ")
    senao 
    escreva("Não pode se aposentar ")


    
  }
}
