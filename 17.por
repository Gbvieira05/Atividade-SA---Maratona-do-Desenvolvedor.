programa {
  funcao inicio() {
    inteiro media, frequencia 

    escreva("Digite a média final do aluno: ")
    leia(media)
    escreva("Digite o percentual de frequência: ")
    leia(frequencia)

    se (media >= 7.0 e frequencia >= 75)
    escreva("Aprovado ")
    senao
    escreva("Reprovado")
    
  }
}
