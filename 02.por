programa {
  funcao inicio() {
    inteiro km
    real combustivel
   
    escreva("Digite a distância percorrida: ")
    leia(km)
    
    escreva("Digite o combustível gasto: ")
    leia(combustivel)

     real soma = km / combustivel

    escreva("O consumo médio do veículo é de: ", soma , "Km")
  }
}
